<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.chleh.smart_pillow4.Alram"
    tools:showIn="@layout/activity_alram">
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <Button
        android:id="@+id/Btn_insert"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="42dp"
        android:layout_marginRight="43dp"
        android:layout_marginTop="12dp"
        android:text="추가"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Btn_sun" />

    <ToggleButton
        android:id="@+id/Btn_wen"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginLeft="124dp"
        android:layout_marginStart="124dp"
        android:text="ToggleButton"
        android:textOff="수"
        android:textOn="수"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.269" />

    <ToggleButton
        android:id="@+id/Btn_thu"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginLeft="172dp"
        android:layout_marginStart="164dp"
        android:text="ToggleButton"
        android:textOff="목"
        android:textOn="목"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.269" />

    <ToggleButton
        android:id="@+id/Btn_sat"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginLeft="244dp"
        android:layout_marginStart="244dp"
        android:text="ToggleButton"
        android:textOff="토"
        android:textOn="토"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.269" />

    <ToggleButton
        android:id="@+id/Btn_mon"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginLeft="44dp"
        android:layout_marginStart="44dp"
        android:checked="false"
        android:textOff="월"
        android:textOn="월"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.269" />

    <ToggleButton
        android:id="@+id/Btn_tue"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginLeft="84dp"
        android:layout_marginStart="84dp"
        android:text="ToggleButton"
        android:textOff="화"
        android:textOn="화"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.269" />

    <ToggleButton
        android:id="@+id/Btn_fri"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginLeft="204dp"
        android:layout_marginStart="204dp"
        android:text="ToggleButton"
        android:textOff="금"
        android:textOn="금"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.269" />

    <ToggleButton
        android:id="@+id/Btn_sun"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginLeft="284dp"
        android:layout_marginStart="284dp"
        android:text="ToggleButton"
        android:textOff="일"
        android:textOn="일"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.269" />

    <TimePicker
        android:id="@+id/timePicker"
        android:layout_width="256dp"
        android:layout_height="100dp"
        android:layout_marginEnd="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginStart="16dp"
        android:accessibilityTraversalBefore="@+id/datePicker"
        android:timePickerMode="spinner"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="1dp" />

    <Button
        android:id="@+id/Btn_delete"
        android:layout_width="85dp"
        android:layout_height="0dp"
        android:text="삭제"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="12dp"
        app:layout_constraintTop_toBottomOf="@+id/Btn_fri" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline"
        app:layout_constraintGuide_begin="16dp"
        android:orientation="vertical" />

    <ListView
        android:id="@+id/List_alram"
        android:layout_width="368dp"
        android:layout_height="242dp"
        android:layout_marginBottom="27dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:choiceMode="singleChoice"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="242dp" />

</android.support.constraint.ConstraintLayout>
