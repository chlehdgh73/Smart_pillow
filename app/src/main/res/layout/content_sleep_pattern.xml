<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.chleh.smart_pillow4.SleepPattern"
    tools:showIn="@layout/activity_sleep_pattern">

    <TextView
        android:id="@+id/efficient_sleep"
        android:layout_width="124dp"
        android:layout_height="39dp"
        android:layout_marginBottom="30dp"
        android:layout_marginRight="79dp"
        android:autoText="false"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/not_sleep"
        android:layout_width="124dp"
        android:layout_height="39dp"
        android:layout_marginBottom="22dp"
        android:layout_marginRight="79dp"
        android:autoText="false"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/efficient_sleep"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/move_sleep"
        android:layout_width="124dp"
        android:layout_height="39dp"
        android:layout_marginBottom="22dp"
        android:layout_marginRight="79dp"
        android:autoText="false"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/not_sleep"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/meta_sleep"
        android:layout_width="124dp"
        android:layout_height="39dp"
        android:layout_marginBottom="22dp"
        android:layout_marginRight="79dp"
        android:autoText="false"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/move_sleep"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/total_sleep"
        android:layout_width="124dp"
        android:layout_height="39dp"
        android:layout_marginBottom="22dp"
        android:layout_marginRight="79dp"
        android:autoText="false"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/meta_sleep"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="124dp"
        android:layout_height="39dp"
        android:layout_marginBottom="30dp"
        android:layout_marginLeft="25dp"
        android:autoText="false"
        android:elegantTextHeight="false"
        android:text="수면 효율성"
        android:textAlignment="center"
        android:textIsSelectable="false"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="124dp"
        android:layout_height="39dp"
        android:layout_marginBottom="22dp"
        android:layout_marginLeft="25dp"
        android:autoText="false"
        android:elegantTextHeight="false"
        android:text="수면 중 깬 시간"
        android:textAlignment="center"
        android:textIsSelectable="false"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="124dp"
        android:layout_height="39dp"
        android:layout_marginBottom="22dp"
        android:layout_marginLeft="25dp"
        android:autoText="false"
        android:elegantTextHeight="false"
        android:text="뒤척인 시간"
        android:textAlignment="center"
        android:textIsSelectable="false"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="124dp"
        android:layout_height="39dp"
        android:layout_marginBottom="22dp"
        android:layout_marginLeft="25dp"
        android:autoText="false"
        android:elegantTextHeight="false"
        android:text="완전 수면시간"
        android:textAlignment="center"
        android:textIsSelectable="false"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Spinner
        android:id="@+id/Date_list"
        android:layout_width="211dp"
        android:layout_height="56dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="16dp"
        app:layout_constraintRight_toLeftOf="@+id/textView"
        android:layout_marginRight="8dp"
        app:layout_constraintHorizontal_bias="0.0" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="106dp"
        android:layout_height="55dp"
        android:layout_marginTop="16dp"
        android:text="일시"
        android:textAlignment="center"
        android:textSize="30sp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="32dp"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="124dp"
        android:layout_height="39dp"
        android:layout_marginBottom="22dp"
        android:layout_marginLeft="25dp"
        android:autoText="false"
        android:elegantTextHeight="false"
        android:text="총 수면시간"
        android:textAlignment="center"
        android:textIsSelectable="false"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintVertical_bias="1.0" />

</android.support.constraint.ConstraintLayout>
